<!doctype html>
<html xmlns:squarespace="">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="initial-scale=1">
    {squarespace-headers}
</head>
<body id="{squarespace.page-id}" class="{squarespace.page-classes} tk-raleway">
<div id="splash">
    <squarespace:query collection="splash">
        {@|apply splash.block}
    </squarespace:query>
</div>
<div id="site">
    <header id="header" role="banner">
        {.section website}
        <h1 id="logo" style="background-image: url('{logoImageUrl}?format=1500w');">
            <a href="/">{siteTitle}</a>
        </h1>
        {.end}
        <squarespace:navigation navigationId="site-nav" template="navigation"/>
    </header>
    <!--Content injection point.-->
    <main id="page" class="content" role="main">
        <section id="hero">
            <squarespace:query collection="hero">
                {@|apply hero.block}
            </squarespace:query>
        </section>
        <section id="our-story">
            <squarespace:query collection="our-story-gallery/bg">
                <div style="height: 100vh;background: url({item.assetUrl}) no-repeat top center scroll; -webkit-background-size: cover; -moz-background-size: cover; -o-background-size: cover; background-size: cover;"></div>
            </squarespace:query>
            <div class="gallery-container">
                <squarespace:query collection="our-story-gallery/emma">
                    <a style="background: url({item.assetUrl}) no-repeat center center scroll; -webkit-background-size: cover; -moz-background-size: cover; -o-background-size: cover; background-size: cover;" id="story-emma" href="#"></a>
                </squarespace:query>
                <squarespace:query collection="our-story-gallery/david">
                    <a style="background: url({item.assetUrl}) no-repeat center center scroll; -webkit-background-size: cover; -moz-background-size: cover; -o-background-size: cover; background-size: cover;" id="story-david" href="#"></a>
                </squarespace:query>
            </div>
        </section>
    </main>
    <footer id="footer" class="footer">
        <squarespace:block-field id="footerBlocks" columns="12"/>
    </footer>
</div>

<!--Combo and minify the scripts when you're not logged in.-->
<squarespace:script src="app.js" combo="{.if authenticatedAccount}false{.or}true{.end}"/>
{squarespace-footers}
</body>
</html> 
