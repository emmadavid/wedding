<!doctype html>
<html>
    <head>
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <meta name="viewport" content="initial-scale=1">

        <script src="https://use.typekit.net/jnq2ers.js"></script>
        <script>try{Typekit.load({ async: true });}catch(e){}</script>

        <!-- Squarespace headers, includes sqs scripts, meta tags, and user content from the code injection tab. -->
        {squarespace-headers}
        <!-- End Squarespace headers. -->
    </head>
    <body id="{squarespace.page-id}" class="{squarespace.page-classes} tk-raleway">
        <div id="site">
            <header id="header" role="banner">
                {.section website}
                    <h1 id="logo">
                        {.section logoImageUrl}
                            <img src="{logoImageUrl}?format=1500w" alt="{siteTitle}" />
                        {.or}
                            {siteTitle}
                        {.end}
                    </h1>
                    {.section siteTagLine}
                        <p id="tagline">{@}</p>
                    {.end}
                {.end}
                <squarespace:navigation navigationId="site-nav" template="navigation" />
            </header>
            <!--Content injection point.-->
            <main id="page" class="content" role="main" data-content-field="main-content">
                {squarespace.main-content}
            </main>

      <!--Footer with open block field.-->
      <footer id="footer" class="footer">
        <squarespace:block-field id="footerBlocks" columns="12" />
      </footer>

    </div>

    <!--Combo and minify the scripts when you're not logged in.-->
    <squarespace:script src="site.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />

    <!--Injection point for tracking scripts and user content from the code injection tab.-->
    {squarespace-footers}
    
  </body>
</html> 
